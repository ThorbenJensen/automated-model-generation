# QUERY GENIOS.DE (PATTERN AND YEAR) - RETURN NUMBER OF RESULTS
articleCount <- function(pattern, year) {
## generate HTML for query
# all german press -> dbShortcut=%3A2%3AALLEQUELLENNEU-203_%3A2%3APRESSEDTL
url <- paste("https://www.genios.de/dosearch?explicitSearch=true&q=",pattern,"&dbShortcut=%3A2%3AALLEQUELLENNEU-203_%3A2%3APRESSEDTL&searchMask=5478&f5562.TI%2CUT%2CDZ%2CBT=&f7363.KO=&f7837.Z2=&f7838.CO%2CC2%2CTA%2CKA%2CVA%2CZ1=&f7839.CT%2CDE%2CZ4=&f7840.Z5=&f7841.Z3=&timeFilterType=on&DT_from=1.1.",year,"&DT_to=31.12.",year,"&timeFilter=0&x=69&y=5",sep="")
# query HTML
library(RCurl)
http <- getURL(url)
# EXTRACTING 'NUMBER OF RESULTS' FROM SEARCHES
# get line
m1 <- regexpr('class="boxResultTitle">[a-zA-Z ()0-9.]*', http, perl=T)
line <- regmatches(http, m1)
# get number
m2 <- regexpr('[0-9]{1,3}[.]?[0-9]{1,3}', line, perl=T)
number <- regmatches(line, m2)
# remove dot
number <- gsub("\\.","",number)
number <- as.numeric(number)
return(number)
}
